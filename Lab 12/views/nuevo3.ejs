    <form action="/capybaras/nuevo3" method="POST">
        <label for="nombre">Nombre de : </label>
        <input type="text" id="nombre" name="nombre" placeholder="Pedro">
        <label for="descripcion">Descripcion: </label>
        <input type="text" id="descripcion" name="descripcion" placeholder="Le gusta la piña">
        <label for="Imagen">Imagen:</label>
        <input type="text" id="imagen" name="imagen" placeholder="URL IMAGEN">
